<% provide(:title, "#{@user.username}") %>
<div class="row">
  <div class="col-md-6 col-md">
    <h2><%= @user.username %></h2>
    <p><em>account created <%= time_ago_in_words(@user.created_at) %> ago</em></p>
    <p><%= link_to "Update your information", edit_user_path(@user) %></p>
  </div>
</div>
